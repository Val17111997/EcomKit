const drawer = document.getElementById('my-cart-drawer');
let blockAutoOpen = false;

// Forcer la mise à jour du contenu du panier
async function forceCartRefresh() {
    try {
        // Récupérer les données du panier
        const response = await fetch('/cart.js');
        const cart = await response.json();
        
        if (cart.item_count === 0) {
            console.log('❌ Panier vide côté serveur');
            return false;
        }
        
        // Chercher le conteneur du panier
        const cartContent = document.getElementById('my-cart-content');
        const itemsWrapper = document.getElementById('cart-items-wrapper');
        
        // Option 1: Déclencher les événements Shopify
        window.dispatchEvent(new CustomEvent('cart:refresh'));
        document.dispatchEvent(new CustomEvent('cart:updated', { detail: cart }));
        
        // Option 2: Forcer un rechargement via fetch de la section
        try {
            const sectionResponse = await fetch(window.location.pathname + '?section_id=cart-drawer');
            const html = await sectionResponse.text();
            
            // Parser et injecter le nouveau contenu
            const parser = new DOMParser();
            const newDoc = parser.parseFromString(html, 'text/html');
            const newContent = newDoc.getElementById('my-cart-content');
            
            if (newContent && cartContent) {
                // Remplacer le contenu avec une micro-animation
                cartContent.style.opacity = '0.8';
                cartContent.innerHTML = newContent.innerHTML;
                
                // Petite animation pour masquer le rechargement
                requestAnimationFrame(() => {
                    cartContent.style.opacity = '1';
                });
            }
        } catch (e) {
            console.log('Section refresh non disponible');
        }
        
        // Option 3: Cliquer sur l'icône panier pour forcer le refresh
        const cartIcon = document.querySelector('[href*="/cart"], [aria-label*="cart" i]');
        if (cartIcon) {
            cartIcon.click();
            // Empêcher la fermeture immédiate
            setTimeout(() => {
                drawer.style.display = 'flex';
                drawer.setAttribute('aria-hidden', 'false');
            }, 10);
        }
        
        return true;
    } catch (error) {
        console.error('Erreur refresh:', error);
        return false;
    }
}

// Ouvrir le drawer avec refresh intelligent
async function openDrawerWithRefresh() {
    console.log('🎯 Ouverture avec refresh...');
    
    // Ouvrir immédiatement (même vide)
    drawer.style.display = 'flex';
    drawer.setAttribute('aria-hidden', 'false');
    drawer.removeAttribute('inert');
    
    // Ajouter une classe pour masquer le contenu temporairement
    drawer.classList.add('cart-loading');
    
    // Injecter du CSS si nécessaire
    if (!document.getElementById('cart-loading-style')) {
        const style = document.createElement('style');
        style.id = 'cart-loading-style';
        style.textContent = `
            .cart-loading #my-cart-content {
                opacity: 0;
                transition: opacity 0.3s ease;
            }
            #my-cart-content {
                transition: opacity 0.3s ease;
            }
        `;
        document.head.appendChild(style);
    }
    
    // Attendre un peu puis forcer le refresh
    setTimeout(async () => {
        const refreshed = await forceCartRefresh();
        
        // Révéler le contenu
        drawer.classList.remove('cart-loading');
        
        if (!refreshed) {
            // Si échec, fermer le drawer
            setTimeout(() => {
                drawer.style.display = 'none';
                drawer.setAttribute('aria-hidden', 'true');
            }, 500);
        }
        
        blockAutoOpen = false;
    }, 100);
}

// Observer pour bloquer les ouvertures automatiques
const observer = new MutationObserver((mutations) => {
    if (blockAutoOpen) {
        mutations.forEach((mutation) => {
            if (mutation.attributeName === 'style' || mutation.attributeName === 'aria-hidden') {
                const isOpening = drawer.style.display === 'flex' || 
                                 drawer.getAttribute('aria-hidden') === 'false';
                
                if (isOpening && !drawer.classList.contains('force-open')) {
                    drawer.style.display = 'none';
                    drawer.setAttribute('aria-hidden', 'true');
                    drawer.setAttribute('inert', '');
                }
            }
        });
    }
});

observer.observe(drawer, { attributes: true });

// Intercepter les ajouts au panier
const originalFetch = window.fetch;
window.fetch = function(...args) {
    const [url, options] = args;
    
    if (url?.includes('/cart/add') && options?.method === 'POST') {
        console.log('🛒 Ajout au panier intercepté');
        blockAutoOpen = true;
        
        return originalFetch.apply(this, args).then(async (response) => {
            if (response.ok) {
                console.log('✅ Produit ajouté');
                
                // Attendre un minimum pour que Shopify traite l'ajout
                setTimeout(() => {
                    openDrawerWithRefresh();
                }, 300);
            } else {
                blockAutoOpen = false;
            }
            
            return response;
        });
    }
    
    return originalFetch.apply(this, args);
};

// Formulaires classiques
document.addEventListener('submit', (e) => {
    if (e.target.action?.includes('/cart/add')) {
        console.log('📝 Formulaire détecté');
        blockAutoOpen = true;
        
        setTimeout(() => {
            openDrawerWithRefresh();
        }, 800);
    }
}, true);

// Désactiver le blocage si drawer fermé
drawer.addEventListener('click', (e) => {
    if (e.target === drawer || e.target.closest('.close')) {
        blockAutoOpen = false;
    }
});

